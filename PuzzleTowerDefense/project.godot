; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PuzzleTowerDefense"
run/main_scene="res://UI_Objects/MainMenu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://Misc/icon.svg"

[aseprite_importers]

aseprite_executable_path="C:/Program Files/Aseprite/Aseprite.exe"

[display]

window/stretch/mode="viewport"
mouse_cursor/custom_image="res://Assets/UI_Assets/01.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg")

[github_to_itch]

config/itch_username=""
config/itch_project_name=""

[rendering]

textures/canvas_textures/default_texture_filter=0
