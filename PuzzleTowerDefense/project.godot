; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PuzzleTowerDefense"
config/tags=PackedStringArray("gitkraken")
run/main_scene="res://Levels/Screens/MainMenu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[aseprite_importers]

aseprite_executable_path="C:/Program Files/Aseprite/Aseprite.exe"

[autoload]

Popups="*res://Globals/Popups.tscn"
EndLevelStats="*res://Custom_Resources/End_Level_Stats.gd"
AudioManager="*res://Systems/AudioManager.tscn"
TowerDataVault="*res://Globals/tower_data_manager.gd"
Signal_Debugger="*res://addons/SignalVisualizer/Debugger/SignalDebugger.gd"
LevelChanger="*res://Globals/level_changer.gd"
GlobalState="*res://Globals/GlobalState.gd"

[debug]

gdscript/warnings/unused_parameter=0

[display]

window/size/viewport_width=1440
window/size/viewport_height=810
window/size/mode=2
window/size/always_on_top=true
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg", "res://addons/SignalVisualizer/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg")

[filesystem]

import/blender/enabled=false

[github_to_itch]

config/itch_username=""
config/itch_project_name=""

